{
  "name": "nacalo",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "commonjs",
  "scripts": {
    "start": "tsc && nodemon ./dist/main.js",
    "build": "tsc",
    "dev": "nodemon",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev:inspect": "nodemon -e ts,json --exec node --inspect=localhost:9222 -r ts-node/register src/main.ts",
    "dev2": "nodemon src/main.ts",
    "typeorm": "npx ts-node ./node_modules/typeorm/cli",
    "migration:run": "npm run typeorm migration:run -- -d ./src/ormconfig.ts",
    "migration:generate": "npm run typeorm -- -d ./src/ormconfig.ts migration:generate ./src/migrations/$npm_config_name",
    "migration:create": "npm run typeorm -- migration:create ./src/migrations/$npm_config_name",
    "migration:revert": "npm run typeorm -- -d ./src/ormconfig.ts migration:revert"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.1.1",
    "@types/dotenv": "^8.2.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "express": "^4.18.2",
    "inversify": "^6.0.1",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.13",
    "tslog": "^4.9.1",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.17",
    "nodemon": "^3.0.1",
    "prisma": "^5.1.1",
    "ts-node": "^10.9.1"
  }
}
